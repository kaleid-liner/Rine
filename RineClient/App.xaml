<Application
    x:Class="RineClient.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:RineClient"
    xmlns:converter="using:RineClient.Converters">
    <Application.Resources>
        <converter:VisibilityConverter x:Key="VisibilityConverter"/>
        <converter:BadgeNumberConverter x:Key="BadgeNumberConverter"/>
        <converter:EnumConverter x:Key="EnumConverter"/>
        <converter:SmartDateTimeConverter x:Key="SmartDateTimeConverter"/>
    </Application.Resources>

</Application>
